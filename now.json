{
  "name": "escapade",
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@now/node"
    },
    {
      "src": "public/dist/bundle.js",
      "use": "@now/static"
    },
    {
      "src": "**/*.html",
      "use": "@now/html-minifier"
    },
    {
      "src": "**/*.css",
      "use": "@now/html-minifier"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.js"
    }
  ]
}